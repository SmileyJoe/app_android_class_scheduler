<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="profile_image_type">
        <enum name="view" value="0" />
        <enum name="edit" value="1" />
    </attr>

    <declare-styleable name="DetailTextView">
        <attr name="title" format="string" />
        <attr name="content" format="reference"/>
        <attr name="transition_prefix" format="string"/>
    </declare-styleable>

    <declare-styleable name="Banner">
        <attr name="message" format="string" />
        <attr name="button_positive" format="string"/>
        <attr name="button_negative" format="string"/>
    </declare-styleable>

    <declare-styleable name="IconTitleView">
        <attr name="icon_title" format="string" />
        <attr name="icon_name" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProfileImage">
        <attr name="profile_image_type"/>
    </declare-styleable>
</resources>